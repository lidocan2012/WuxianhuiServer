<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.wifihi.persistance">
	<class
		name="Ordermanage"
		table="ordermanage"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="OrderId"
		>
			<generator class="identity"/>
		</id>

		<property
			name="OrderDate"
			column="OrderDate"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="OrderTime"
			column="OrderTime"
			type="time"
			not-null="false"
			length="8"
		/>
		<property
			name="Expenditure"
			column="Expenditure"
			type="big_decimal"
			not-null="false"
			length="8"
		/>
		<property
			name="Status"
			column="Status"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="UserID"
			column="UserID"
			class="User"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="WSPUser"
			column="WSPUser"
			class="Wspuser"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="TableID"
			column="TableID"
			class="Tablemanage"
			not-null="false"
		>
		</many-to-one>
	

		<set name="Orderdetails" inverse="true">
			<key column="OrderId"/>
			<one-to-many class="Orderdetail"/>
		</set>


	</class>	
</hibernate-mapping>